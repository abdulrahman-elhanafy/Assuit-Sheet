#include <iostream>
using namespace std;

int main() {
    long long T;
    cin >> T;

    while (T--){
        long long N;
        cin >> N;

        long long fact = 1;
        for (long long i = 1; i <= N; i++)
            fact *= i;
        
        cout << fact << endl;

    }

    return 0;
}
