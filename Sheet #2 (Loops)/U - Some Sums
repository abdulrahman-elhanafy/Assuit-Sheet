#include <iostream>
using namespace std;
 
int main() {
    int N, A, B;
    cin >> N >> A >> B;
 
    int totalSum = 0;
 
    for (int i = 1; i <= N; i++) {
        int currentNum = i; 
        int sum = 0; 
 
        while (currentNum > 0) {
            sum += currentNum % 10; 
            currentNum /= 10;              
        }
 
        if (sum >= A && sum <= B) {
            totalSum += i; 
        }
    }
    cout << totalSum << endl;
 
    return 0;
}
