#include <iostream>
#include <cmath>

using namespace std;

int main() {
    int T;
    cin >>T;

    while (T--) {
        int N;
        cin >> N;

        int count = 0;
        int temp = N;

        while (temp > 0) {
            if (temp % 2 == 1) {
                count++;
            }
            temp /= 2;
        }

        int result = 0;
        for (int i = 0; i < count; i++) {
            result += pow(2, i);
        }
        cout << result << endl;
    }
    return 0;
}
