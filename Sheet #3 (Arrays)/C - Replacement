#include <iostream>
using namespace std;

int main()
{
    int n = 0;
    cin >> n;
    int arr[n];
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }

    for (int i = 0; i < n; i++)
    {
        if (arr[i] > 0)
        {
            arr[i] = 1;
        }
        else if (arr[i] < 0)
        {
            arr[i] = 2;
        }
        cout << arr[i] << " ";
    }
    return 0;
}

// #####################################################################3

#include <iostream>
using namespace std;

int main()
{
    int n = 0;
    cin >> n;
    int arr[n];
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
        arr[i] = (arr[i] > 0) ? 1 : (arr[i] < 0) ? 2 : 0;
    }

    for (int Array : arr)
    {
        cout << Array << " ";
    }

    return 0;
}
