#include <iostream>
using namespace std;

int main()
{
    int N = 0;
    cin >> N;
    int A[N];
    int B[N];
    for (int i = 0; i < N; i++)
    {
        cin >> A[i];
    }

    for (int i = N - 1; i >= 0; i--)
    {
        cout << A[i] << " ";
    }

    return 0;
}



// ##############################################


#include <iostream>
using namespace std;

int main()
{
    int N = 0;
    cin >> N;
    int A[N];
    int B[N];
    for (int i = 0; i < N; i++)
    {
        cin >> A[i];
    }
    for (int i = 0; i < N; i++)
    {
        B[i] = A[(N - 1) - i];
    }

    for (int i = 0; i < N; i++)
    {
        cout << B[i] << " ";
    }

    return 0;
}
