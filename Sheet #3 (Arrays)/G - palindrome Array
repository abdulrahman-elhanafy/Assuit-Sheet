#include <iostream>
#include <algorithm>
using namespace std;

int main()
{
    int size = 0;
    cin >> size;
    int arr[size];
    int rvxarr[size];
    for (int i = 0; i < size; i++)
    {
        cin >> arr[i];
    }
    for (int i = 0; i < size; i++)
    {
        rvxarr[i] = arr[size - 1 - i];
    }
    bool theSame = true;

    for (int i = 0; i < size; i++)
    {
        if (arr[i] != rvxarr[i])
        {
            theSame = false;
            break;
        }
    }

    if (theSame)
        cout << "YES";
    else
        cout << "NO";

    return 0;
}
